NEXT_PUBLIC_CONTACT_PHONE=+972 53 370 0551
NEXT_PUBLIC_WHATSAPP_NUMBER=+972533700551

# Social Media URLs
NEXT_PUBLIC_FACEBOOK_URL=https://www.facebook.com/webyarden
NEXT_PUBLIC_INSTAGRAM_URL=https://www.instagram.com/webyarden
NEXT_PUBLIC_LINKEDIN_URL=https://www.linkedin.com/company/web-yarden

# Mode staging
NODE_ENV=production
